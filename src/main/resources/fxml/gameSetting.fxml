<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="Controller.gameSettingController"
            prefHeight="400.0" prefWidth="600.0" stylesheets="@css/gameSetting.css" styleClass="gameSetting">
    <Button fx:id="exit" onAction="#close" text="×" />
    <Button fx:id="selectDir" onAction="#selectDir" text="选择目录"/>
    <TextField fx:id="dir" translateY="30"/>
    <Label fx:id="memoryLabel" layoutY="60" text="内存大小" translateY="80"/>
    <Slider fx:id="memorySlider" translateY="100"/>
</AnchorPane>
