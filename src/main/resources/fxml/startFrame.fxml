<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<?import javafx.scene.text.Text?>
<AnchorPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="Controller.StartFrameController"
            prefHeight="500.0" prefWidth="800.0" stylesheets="@css/startFrame.css">
    <AnchorPane fx:id="version" prefHeight="50">
        <Button fx:id="selectDirBtn" text="未找到Minecraft版本" onAction="#selectDirBtn" prefWidth="150" prefHeight="50"/>
        <ComboBox fx:id="versionChoiceBox" prefWidth="150" prefHeight="50"/>
    </AnchorPane>
    <TextField fx:id="username" prefWidth="100" promptText="请输入用户名" prefHeight="20" translateY="80"/>
    <Slider fx:id="memorySlider" translateY="110"/>
    <Text fx:id="memorySliderData" translateY="110" translateX="100"/>
    <Button fx:id="download" text="下载Minecraft" translateY="130" onAction="#downloadMC"/>
    <Button fx:id="startBtn" text="启动" onAction="#startBtn" translateY="150"/>

</AnchorPane>
